{
    "name": "spork/planning",
    "description": "The planning package for Spork",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "5355937+austinkregel@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Spork\\Planning\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spork\\Planning\\PlanningServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spork\\Planning\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "homepage": "https://github.com/spork-app/news",
    "keywords": [
        "spork-plugins"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}